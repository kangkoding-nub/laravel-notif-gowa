{
  "name": "kangkoding/laravel-notif-gowa",
  "description": "GOWA Notifications channel untuk laravel versi 12",
  "keywords": [
    "laravel",
    "notifications",
    "gowa"
  ],
  "homepage": "https://github.com/kangkoding-nub/laravel-notif-gowa",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "kangKoding Nub",
      "role": "Developer",
      "email": "kangkoding.nub@gmail.com",
      "homepage": "https://github.com/kangkoding-nub"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/queue": "^11.0|^12.0",
    "illuminate/notifications": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\Gowa\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\Gowa\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\Gowa\\GowaServiceProvider"
      ]
    }
  }
}
